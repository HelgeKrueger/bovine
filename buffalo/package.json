    "@mui/material": "^5.11.5",
    "dexie": "^3.2.3",
    "dexie-react-hooks": "^1.1.1",
    "esbuild": "^0.17.3",
    "event-source-polyfill": "^1.0.31",
    "kramed": "^0.5.6",
    "marked": "^4.2.12",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.3",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.7.0",
    "react-simple-pull-to-refresh": "^1.3.3",
    "react-swipeable": "^7.0.0",
    "uuid": "^9.0.0"
